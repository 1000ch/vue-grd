{
  "name": "vue-grd",
  "version": "0.6.0",
  "description": "Vue port of grd, a CSS grid framework using flexbox.",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rollup -c rollup.config.js && uglifyjs -o dist/index.js dist/index.js"
  },
  "main": "dist/index.js",
  "lint-staged": {
    "*.{vue|js}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/1000ch/vue-grd.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "1000ch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1000ch/vue-grd/issues"
  },
  "homepage": "https://github.com/1000ch/vue-grd#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "rollup": "^0.57.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-vue": "^3.0.0",
    "uglify-es": "^3.3.9",
    "vue-template-compiler": "^2.5.16"
  }
