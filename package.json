  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1000ch/vue-grd/issues"
  },
  "homepage": "https://github.com/1000ch/vue-grd#readme",
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "rollup": "^0.57.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-template-compiler": "^2.5.16"
  }
