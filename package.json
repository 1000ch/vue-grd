{
  "name": "vue-grd",
  "version": "1.2.2",
  "description": "Simple, Light-weight and Flexible Vue.js component for grid layout.",
  "scripts": {
    "prepublish": "npm run build",
    "test": "jest",
    "build": "rollup -c rollup.config.js"
  },
  "main": "dist/vue-grd.common.js",
  "module": "dist/vue-grd.esm.js",
  "browser": "dist/vue-grd.js",
  "lint-staged": {
    "*.{vue|js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "/test/.+\\.spec\\.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      ".*\\.(js)$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/1000ch/vue-grd.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "1000ch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1000ch/vue-grd/issues"
  },
  "homepage": "https://github.com/1000ch/vue-grd#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.4",
    "vue": "^2.6.11",
    "vue-jest": "github:vuejs/vue-jest",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  }
}
